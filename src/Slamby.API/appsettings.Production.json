{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "SlambyApi": {
    "BaseUrlPrefix": "",
    "ElasticSearch": {
      "Uris": [ 
        "http://elasticsearch:9200/"
      ]
    },
    "Parallel": { },
    "Serilog": {
      "Output": "/Slamby/Logs",
      "RetainedFileCountLimit": 14,
      "MinimumLevel": "Information"
    },
    "Resources": {
      "LogPath": "/app",
      "RefreshInterval": 5000
    },
    "Redis": {
      "Enabled": true,
      "Configuration": "redis,abortConnect=false,ssl=false,syncTimeout=30000"
    }
  }
}
